<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.bsp.MVO.mapper.PackageInfoMapper">

    <resultMap type="PackageInfo" id="PckMap">
        <result property="pckId" column="PCK_ID" jdbcType="INTEGER"/>
        <result property="warId" column="WAR_ID" jdbcType="INTEGER"/>
        <result property="proId" column="PRO_ID" jdbcType="INTEGER"/>
        <result property="typeCd" column="TYPE_CD" jdbcType="VARCHAR"/>
        <result property="width" column="WIDTH" jdbcType="NUMERIC"/>
        <result property="height" column="HEIGHT" jdbcType="NUMERIC"/>
        <result property="length" column="LENGTH" jdbcType="NUMERIC"/>
        <result property="weight" column="WEIGHT" jdbcType="NUMERIC"/>
        <result property="createdBy" column="CREATED_BY" jdbcType="VARCHAR"/>
        <result property="creationDate" column="CREATION_DATE" jdbcType="TIMESTAMP"/>
        <result property="lastUpdateBy" column="LAST_UPDATE_BY" jdbcType="VARCHAR"/>
        <result property="lastUpdateDate" column="LAST_UPDATE_DATE" jdbcType="TIMESTAMP"/>
        <result property="callCnt" column="CALL_CNT" jdbcType="INTEGER"/>
        <result property="remark" column="REMARK" jdbcType="VARCHAR"/>
        <result property="stsCd" column="STS_CD" jdbcType="VARCHAR"/>
        <result property="heavyCargo" column="HEAVY_CARGO" jdbcType="VARCHAR"/>
        <result property="logisticsCompany" column="LOGISTICS_COMPANY" jdbcType="VARCHAR"/>
    </resultMap>

    <!--查询单个-->
    <select id="getById" resultMap="PckMap">
        select
          PCK_ID, WAR_ID, PRO_ID, TYPE_CD, WIDTH, HEIGHT, LENGTH, WEIGHT, CREATED_BY, CREATION_DATE, LAST_UPDATE_BY, LAST_UPDATE_DATE, CALL_CNT, REMARK, STS_CD, HEAVY_CARGO, LOGISTICS_COMPANY
        from bsp.pck
        where PCK_ID = #{pckId}
    </select>


    <!--通过实体作为筛选条件查询-->
    <select id="getAll" resultMap="PckMap">
        select
        PCK_ID, WAR_ID, PRO_ID, TYPE_CD, WIDTH, HEIGHT, LENGTH, WEIGHT, CREATED_BY, CREATION_DATE, LAST_UPDATE_BY,
        LAST_UPDATE_DATE, CALL_CNT, REMARK, STS_CD, HEAVY_CARGO, LOGISTICS_COMPANY
        from bsp.pck
        <where>
            <if test="pckId != null">
                and PCK_ID = #{pckId}
            </if>
            <if test="warId != null">
                and WAR_ID = #{warId}
            </if>
            <if test="proId != null">
                and PRO_ID = #{proId}
            </if>
            <if test="typeCd != null and typeCd != ''">
                and TYPE_CD = #{typeCd}
            </if>
            <if test="width != null">
                and WIDTH = #{width}
            </if>
            <if test="height != null">
                and HEIGHT = #{height}
            </if>
            <if test="length != null">
                and LENGTH = #{length}
            </if>
            <if test="weight != null">
                and WEIGHT = #{weight}
            </if>
            <if test="createdBy != null and createdBy != ''">
                and CREATED_BY = #{createdBy}
            </if>
            <if test="creationDate != null">
                and CREATION_DATE = #{creationDate}
            </if>
            <if test="lastUpdateBy != null and lastUpdateBy != ''">
                and LAST_UPDATE_BY = #{lastUpdateBy}
            </if>
            <if test="lastUpdateDate != null">
                and LAST_UPDATE_DATE = #{lastUpdateDate}
            </if>
            <if test="callCnt != null">
                and CALL_CNT = #{callCnt}
            </if>
            <if test="remark != null and remark != ''">
                and REMARK = #{remark}
            </if>
            <if test="stsCd != null and stsCd != ''">
                and STS_CD = #{stsCd}
            </if>
            <if test="heavyCargo != null and heavyCargo != ''">
                and HEAVY_CARGO = #{heavyCargo}
            </if>
            <if test="logisticsCompany != null and logisticsCompany != ''">
                and LOGISTICS_COMPANY = #{logisticsCompany}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="pckId" useGeneratedKeys="true">
        insert into bsp.pck(WAR_ID, PRO_ID, TYPE_CD, WIDTH, HEIGHT, LENGTH, WEIGHT, CREATED_BY, CREATION_DATE, LAST_UPDATE_BY, LAST_UPDATE_DATE, CALL_CNT, REMARK, STS_CD, HEAVY_CARGO, LOGISTICS_COMPANY)
        values (#{warId}, #{proId}, #{typeCd}, #{width}, #{height}, #{length}, #{weight}, #{createdBy}, #{creationDate}, #{lastUpdateBy}, #{lastUpdateDate}, #{callCnt}, #{remark}, #{stsCd}, #{heavyCargo}, #{logisticsCompany})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update bsp.pck
        <set>
            <if test="warId != null">
                WAR_ID = #{warId},
            </if>
            <if test="proId != null">
                PRO_ID = #{proId},
            </if>
            <if test="typeCd != null and typeCd != ''">
                TYPE_CD = #{typeCd},
            </if>
            <if test="width != null">
                WIDTH = #{width},
            </if>
            <if test="height != null">
                HEIGHT = #{height},
            </if>
            <if test="length != null">
                LENGTH = #{length},
            </if>
            <if test="weight != null">
                WEIGHT = #{weight},
            </if>
            <if test="createdBy != null and createdBy != ''">
                CREATED_BY = #{createdBy},
            </if>
            <if test="creationDate != null">
                CREATION_DATE = #{creationDate},
            </if>
            <if test="lastUpdateBy != null and lastUpdateBy != ''">
                LAST_UPDATE_BY = #{lastUpdateBy},
            </if>
            <if test="lastUpdateDate != null">
                LAST_UPDATE_DATE = #{lastUpdateDate},
            </if>
            <if test="callCnt != null">
                CALL_CNT = #{callCnt},
            </if>
            <if test="remark != null and remark != ''">
                REMARK = #{remark},
            </if>
            <if test="stsCd != null and stsCd != ''">
                STS_CD = #{stsCd},
            </if>
            <if test="heavyCargo != null and heavyCargo != ''">
                HEAVY_CARGO = #{heavyCargo},
            </if>
            <if test="logisticsCompany != null and logisticsCompany != ''">
                LOGISTICS_COMPANY = #{logisticsCompany},
            </if>
        </set>
        where PCK_ID = #{pckId}
    </update>

    <!--通过主键删除-->
    <delete id="delete">
        delete from bsp.pck where PCK_ID = #{pckId}
    </delete>

</mapper>